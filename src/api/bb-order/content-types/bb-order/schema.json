{
  "kind": "collectionType",
  "collectionName": "bb_orders",
  "info": {
    "singularName": "bb-order",
    "pluralName": "bb-orders",
    "displayName": "BB - Order",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "users_permissions_user": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    },
    "userName": {
      "type": "string"
    },
    "phone": {
      "type": "string"
    },
    "address": {
      "type": "string"
    },
    "additionalInfo": {
      "type": "text"
    },
    "payment": {
      "type": "enumeration",
      "enum": [
        "cash",
        "bank"
      ]
    },
    "total": {
      "type": "decimal"
    },
    "products": {
      "type": "component",
      "repeatable": true,
      "component": "cart.cart-items"
    }
  }
}
